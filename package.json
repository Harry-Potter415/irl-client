    "@material-ui/icons": "^1.1.1",
    "@material-ui/lab": "^4.0.0-alpha.57",
    "@material-ui/pickers": "^3.2.10",
    "@stripe/react-stripe-js": "^1.2.2",
    "@stripe/stripe-js": "^1.12.0",
    "algoliasearch": "^3.35.1",
    "apexcharts": "^3.24.0",
    "append-query": "^2.1.0",
    "axios": "^0.21.1",
    "copy-to-clipboard": "^3.3.1",
    "create-react-app": "^2.1.8",
    "create-react-class": "^15.7.0",
    "custom-event-polyfill": "^1.0.7",
    "debounce-async": "0.0.2",
    "detect-passive-events": "^1.0.5",
    "graphql-request": "^1.8.2",
    "helmet": "^3.23.3",
    "immutable": "^3.8.2",
    "jsonapi-normalizer": "^2.0.1",
    "jwt-decode": "^2.2.0",
    "lodash": "^4.17.20",
    "material-ui-flat-pagination": "^3.2.1",
    "mem": "^5.1.1",
    "moment": "^2.29.1",
    "node-sass": "^4.12.0",
    "pluralize": "^7.0.0",
    "pure-react-carousel": "^1.27.6",
    "query-string": "^6.13.8",
    "react": "^16.14.0",
    "react-apexcharts": "^1.3.7",
    "react-dom": "^16.14.0",
    "react-dropzone": "^4.3.0",
    "react-error-boundary": "^1.2.5",
    "react-ga": "^2.7.0",
    "react-google-maps": "^9.4.5",
    "react-helmet": "^5.2.1",
    "react-https-redirect": "^1.1.0",
    "react-image": "^2.4.0",
    "react-instantsearch": "^5.7.0",
    "react-instantsearch-dom": "^5.7.0",
    "react-intercom": "^1.0.15",
    "react-medium-image-zoom": "^3.1.2",
    "react-moment": "^0.7.9",
    "react-redux": "^7.2.2",
    "react-router-dom": "^4.2.2",
    "react-router-redux": "next",
    "react-scripts": "3.3.1",
    "react-scroll": "^1.8.1",
    "react-select": "^2.4.4",
    "react-sticky-fill": "^0.8.4",
    "react-swipeable-views": "^0.13.9",
    "react-use-form-state": "^0.11.0",
    "react-waypoint": "^9.0.3",
    "redux": "^3.7.2",
    "redux-form": "^7.4.3",
